  },
  "homepage": "https://github.com/wikimedia/restbase-mod-table-spec",
  "dependencies": {
    "routeswitch": "~0.6.3",
    "cassandra-uuid": "^0.1.0",
    "js-yaml": "^3.12.0"
  },
  "devDependencies": {
    "bluebird": "~3.5.2",
    "coveralls": "3.0.2",
    "extend": "^3.0.2",
    "istanbul": "0.4.5",
    "mocha-lcov-reporter": "1.3.0",
    "mocha": "5.2.0",
    "eslint-config-wikimedia": "^0.10.0",
    "eslint-plugin-jsdoc": "^3.9.1",
    "eslint-plugin-json": "^1.2.1"
  }
